# -- Mutex lock/unlock --
mutex_lock() {
  name=$1
  lockf="$STATE_ROOT/mutex/$name.lock"
  if [ -f "$lockf" ]; then
    printf "BUSY\n"; return 1
  fi
  echo $$ > "$lockf"
  printf "LOCKED\n"
}
mutex_unlock() {
  name=$1
  rm -f "$STATE_ROOT/.tmp/mutex/$name.lock" 2>/dev/null || true
  printf "UNLOCKED\n"
}

