SVC)
            num=$2
            if echo "$num" | grep -q '^#'; then
                svc_num=${num#\#}
            else
                svc_num=$num
            fi
            case "$svc_num" in
                0)
                    echo "SVC 0: Allocated: 0x9000"
                    ;;
                1)
                    echo "SVC 1: Dumping registers"
                    echo "=== Registers ==="
                    for reg in r0 r1 r2 r3 r4 r5 r6 r7; do
                        val=$(getreg "$reg")
                        echo "$reg = $val"
                    done
                    ;;
                2)
                    echo "SVC 2: Halt"
                    exit 0
                    ;;
                3) echo "SVC 3: MMIO: INITDDR RESET"
                 ;;
                 *)
                    echo "Unknown SVC number: $svc_num"
                    ;;
            esac
            ;;
NOP)
