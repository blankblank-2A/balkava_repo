        r=$(echo "$args" | awk '{print $1}')
        imm=$(echo "$args" | awk '{print $2}')
        shift=$(echo "$args" | awk '{print $3}')
        old=$(getreg "$r")
        mask=$((~(0xFFFF << shift)))
        val=$(( (old & mask) | (imm << shift) ))
        setreg "$r" "$val"
        ;;
    ADRP) # ADRP rDST ADDR
        r=$(echo "$args" | awk '{print $1}')
        addr=$(echo "$args" | awk '{print $2}')
